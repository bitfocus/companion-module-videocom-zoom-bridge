{
	"name": "videocom-zoom-bridge",
	"version": "1.0.0",
	"api_version": "1.0.0",
	"main": "dist/index.js",
	"repository": "https://github.com/bitfocus/companion-module-videocom-zoom-bridge.git",
	"author": "JeffreyDavidsz <jeffrey.davidsz@vicreo.eu>",
	"license": "MIT",
	"scripts": {
		"dev": "yarn build:watch",
		"build": "npx rimraf dist && npx typescript@~4.5.5 -p tsconfig.build.json",
		"build:main": "tsc -p tsconfig.build.json",
		"build:watch": "tsc -p tsconfig.build.json --watch",
		"lint": "eslint . --ext .ts",
		"postinstall": "yarn build",
		"test": "jest"
	},
	"bugs": {
		"url": "https://github.com/bitfocus/companion-module-videocom-zoom-bridge/issues"
	},
	"manufacturer": "videocom",
	"product": [
		"Zoom Bridge"
	],
	"shortname": "zoombridge",
	"description": "Zoom Bridge NDI plugin for companion ",
	"keywords": [
		"NDI",
		"zoom"
	],
	"dependencies": {
		"@types/lodash": "^4.14.178",
		"lodash": "^4.17.21",
		"urllib": "^3.0.4"
	},
	"devDependencies": {
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.12",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.4.7",
		"lint-staged": "^12.3.2",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.3",
		"typescript": "^4.5.5"
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		],
		"*.{ts}": [
			"yarn lint --fix --",
			"git add"
		]
	},
	"eslintIgnore": [
		"/tests/*.test.ts"
	]
}
